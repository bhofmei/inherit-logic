{
  "name": "inherit-logic",
  "version": "2.0.0",
  "description": "Web application using computer simulated experiments to learn about genetics",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "back:docs": "./add-doc/gen-backend-docs.sh",
    "ng:docs": "./node_modules/.bin/compodoc public -t -c .compodocrc.json",
    "docs": "yarn run back:docs; yarn run ng:docs",
    "webpack": "webpack --watch --config webpack.dev.js",
    "webpack:p": "webpack --config webpack.prod.js",
    "app": "node index",
    "start:w": "concurrently \"npm run webpack\" \"npm run app\" ",
    "start:p": "concurrently \"npm run webpack:p\" \"npm run app\" ",
    "test:back": "NODE_ENV=test mocha --recursive --reporter spec -t 5000 app/tests/",
    "test:server": "NODE_ENV=test mocha --reporter spec -t 5000 app/tests/",
    "test:cricket": "NODE_ENV=test mocha --reporter spec app/tests/cricket",
    "test:cricket-genetics": "NODE_ENV=test mocha --reporter spec app/tests/genetics/cricket",
    "test:mendelpede": "NODE_ENV=test mocha --reporter spec app/tests/mendelpede",
    "test:ng": "karma start",
    "cov:back": "NODE_ENV=test nyc --reporter=json-summary mocha --recursive --reporter spec -t 5000 app/tests/",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "keywords": [
    "genetics",
    "angular"
  ],
  "author": "Brigitte Hofmeister",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "^5.0.0",
